{
    "name": "spatie/laravel-database-cleanup",
    "description": "This package deletes expired records in a database.",
    "keywords": [
        "spatie",
        "laravel-database-cleanup"
    ],
    "homepage": "https://github.com/spatie/laravel-database-cleanup",
    "license": "MIT",
    "authors": [
        {
            "name": "Jolita Grazyte",
            "email": "jolita@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.0",
        "illuminate/support": "^5.2",
        "nesbot/carbon": "^1.0",
        "nikic/php-parser":"^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "orchestra/testbench":"^3.2"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\DatabaseCleanup\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\DatabaseCleanup\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
